		api:namespace "http://location.data.gov.uk/so/ef/SamplingPoint/bwsp.eaew/";
		api:prefix	  "bswp";
	];
	api:maxPageSize "100";
	api:defaultPageSize "10" ;
  api:sparqlEndpoint <http://services.data.gov.uk/environment/sparql> ;
  api:defaultFormatter t:HtmlFormatter ;
