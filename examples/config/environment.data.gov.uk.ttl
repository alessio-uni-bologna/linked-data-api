		api:namespace "http://environment.data.gov.uk/def/bathing-water-quality/";
		api:prefix	  "bwq_def";
	];
	api:prefixMapping [ 
		api:namespace "http://environment.data.gov.uk/id/bathing-water/";
		api:prefix	  "bw_i";
	];
