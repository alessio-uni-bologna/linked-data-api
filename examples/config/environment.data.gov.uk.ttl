## In-Season Samples by Sampling Point
t:inSeasonAssessments
  a	api:ListEndpoint;
  api:uriTemplate			"/environment/doc/bathing-water-quality/in-season/point/{bwspid}/all" ;
  api:exampleRequestPath	"/environment/doc/bathing-water-quality/in-season/point/03600/all" ;
  api:viewer
	t:inSeasonAssessmentViewer,
	t:inSeasonAssessmentValuesViewer,
	t:inSeasonClassificationViewer,
	t:inSeasonColiformCountViewer,
	t:inSeasonStreptococciCountViewer,
	t:inSeasonEntrovirusCountViewer,
	t:inSeasonSalmonellaPresentViewer; 
  api:defaultPageSize		"10";
  api:selector [
    api:filter "type=SampleAssessment&bwq_samplingPoint.samplePointNotation={bwspid}";
## Either this long 'where' clause 
##    api:where  "?slice a bwq:BySamplingPointSlice; bwq:samplingPoint [ ef_def:samplePointNotation '?bwspid'^^ef_def:samplePointCode ]; qb:observation ?item . ?item bwq:sampleDateTime [time:inXSDDateTime ?x_dt] .";
## or the short filter and where clause below
    api:sort "sampleDateTime.inXSDDateTime";
  ] ;
  api:defaultViewer	api:describeViewer;
#  api:defaultViewer	t:inSeasonAssessmentValuesViewer;
  .
  
## Lastest Samples
t:latestInSeasonAssessments
  a	api:ListEndpoint;
  api:uriTemplate			"/environment/doc/bathing-water-quality/in-season/latest" ;
  api:exampleRequestPath	"/environment/doc/bathing-water-quality/in-season/latest" ;
  api:defaultPageSize		"10";
  api:viewer
	t:inSeasonAssessmentViewer,
	t:inSeasonAssessmentValuesViewer,
	t:inSeasonClassificationViewer,
	t:inSeasonColiformCountViewer,
	t:inSeasonStreptococciCountViewer,
	t:inSeasonEntrovirusCountViewer,
	t:inSeasonSalmonellaPresentViewer; 
  api:selector [
    api:where
    """
    bwq-iss:latest
       qb:observation ?item .
    ?item bwq:sampleWeek [ rdfs:label ?week_l];
          bwq:bathingWater [ skos:prefLabel ?name ] .            
    """ ;
    api:orderBy "?week_l ?name";
  ] ;
  api:defaultViewer	t:inSeasonAssessmentViewer;
  .

## Lastest Samples
t:latestInSeasonAssessments2
  a	api:ListEndpoint;
  api:uriTemplate			"/environment/doc/bathing-water-quality/in-season/latest" ;
  api:exampleRequestPath	"/environment/doc/bathing-water-quality/in-season/latest" ;
  api:defaultPageSize		"20";
  api:selector [
    api:where
    """
    ?slice a bwq:BySamplingPointYearSlice;
             bwq:samplingPoint   ?sp;
            .  
           
    OPTIONAL {
      ?slice2 a bwq:BySamplingPointYearSlice;
              bwq:samplingPoint ?sp;
              .
      FILTER(STR(?slice2)>STR(?slice))
    }
    FILTER (!bound(?slice2))
               
    ?slice qb:observation ?item .   
    
    OPTIONAL {
      ?slice qb:observation ?item2 .
      FILTER(STR(?item2)>STR(?item))
    }
    FILTER(!bound(?item2))
    
    ?item bwq:bathingWater ?bw;
          bwq:sampleWeek   ?week;
          .
    ?bw   skos:prefLabel   ?name .
    ?week rdfs:label	   ?week_l.
    """ ;
    api:orderBy "?week_l ?name";
    
  ] ;
  api:defaultViewer	t:inSeasonAssessmentViewer;
  .
 
# 
# Latest in-season sample assessment by sampling point
# that relies on a single 'constructed' LatestSampleSlice (bwq-iss:latest)
# being present in the processed data
#
t:latestInSeasonAssessment2
  a	api:ListEndpoint;
  api:uriTemplate			"/environment/doc/bathing-water-quality/in-season/point/{bwspid}/latest" ;
  api:exampleRequestPath	"/environment/doc/bathing-water-quality/in-season/point/19100/latest" ;
  api:defaultPageSize		"1";
  api:maxPageSize 			"1";
  api:selector [
  		api:filter "type=SampleAssessment,bwq_samplingPoint.samplePointNotation=?bwspid";
  		api:where  "bwq-iss:latest qb:observation ?item."
  ];
  api:defaultViewer	t:inSeasonAssessmentViewer;
  . 
  
# 
# Latest in-season sample assessment by sampling point based soley on the raw data.
#
t:latestInSeasonAssessment
  a	api:ListEndpoint;
  api:uriTemplate			"/environment/doc/bathing-water-quality/in-season/point/{bwspid}/latest" ;
  api:exampleRequestPath	"/environment/doc/bathing-water-quality/in-season/point/19100/latest" ;
  api:defaultPageSize		"1";
  api:maxPageSize 			"1";
  api:selector [
  		api:filter "type=SampleAssessment,bwq_samplingPoint.samplePointNotation=?bwspid";
  		api:sort	"-sampleDateTime";
  ];
  api:defaultViewer	t:inSeasonAssessmentViewer;
  . 
  
